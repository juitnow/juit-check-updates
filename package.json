{
  "name": "@juitnow/cmd-check-updates",
  "version": "1.3.0",
  "description": "Small and fast utility to update package dependencies",
  "author": "Team Juit <developers@juit.com>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "check-updates": "./check-updates.js"
  },
  "scripts": {
    "build": "npm run lint -- --cache && npm run compile",
    "bump": "bash -c 'npm --no-git-tag-version version ${1:-patch}' --",
    "compile": "tsc || false",
    "dev": "npx nodemon -i dist -e js,json,ts -x npm run build",
    "lint": "eslint --ext .js --ext .ts .",
    "lintfix": "npm run lint -- --fix",
    "update-node-base": "npx @juitnow/cmd-node-base --release --typescript --replace && npm run updates",
    "updates": "npm install && node ./check-updates.js && rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "ini": "^2.0.0",
    "npm-registry-fetch": "^11.0.0",
    "semver": "^7.3.5",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/glob": "^7.2.0",
    "@types/ini": "^1.3.31",
    "@types/mocha": "<8.0.0",
    "@types/node": "<13.0.0",
    "@types/npm-registry-fetch": "^8.0.2",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-tsdoc": "^0.2.14",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juitnow/cmd-check-updates.git"
  },
  "bugs": {
    "url": "https://github.com/juitnow/cmd-check-updates/issues"
  },
  "homepage": "https://github.com/juitnow/cmd-check-updates#readme"
}
