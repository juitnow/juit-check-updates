{
  "name": "@juit/check-updates",
  "version": "3.0.2",
  "type": "module",
  "description": "Small and fast utility to update package dependencies",
  "author": "Team Juit <developers@juit.com>",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "bin": {
    "check-updates": "./dist/main.mjs"
  },
  "scripts": {
    "build": "plug",
    "coverage": "plug coverage",
    "dev": "plug coverage -w src -w test",
    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "dependencies": {
    "ini": "^4.1.1",
    "npm-registry-fetch": "^16.0.0",
    "semver": "^7.5.4",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@plugjs/build": "^0.5.6",
    "@types/ini": "^1.3.31",
    "@types/npm-registry-fetch": "^8.0.5",
    "@types/semver": "^7.5.3",
    "@types/yargs-parser": "^21.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juitnow/juit-check-updates.git"
  },
  "bugs": {
    "url": "https://github.com/juitnow/juit-check-updates/issues"
  },
  "homepage": "https://github.com/juitnow/juit-check-updates#readme",
  "files": [
    "*.md",
    "dist/",
    "src/"
  ],
  "directories": {
    "test": "test"
  },
  "keywords": [
    "check",
    "dependencies",
    "management",
    "ncu",
    "npm",
    "package.json",
    "updater",
    "updates",
    "upgrades",
    "version"
  ]
}
